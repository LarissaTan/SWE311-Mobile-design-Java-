<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".temp_head.CartoonsFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_alignParentStart="true">

        <com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
            android:id="@+id/youtube_player_view_cartoon"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <TextView
            android:id="@+id/cartoon_video_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Learns How To Make Pancakes!"
            android:fontFamily="@font/abeezee_regular"
            android:textSize="24sp"
            android:textColor="#1E1F20"
            android:layout_marginTop="20dp"
            android:layout_marginStart="15dp"/>



    </LinearLayout>

</FrameLayout>